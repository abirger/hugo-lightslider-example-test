        &lt;script&gt;
        $(document).ready(function() {
            $(&#39;#lightslider_example_slider1&#39;).lightSlider({
               gallery: true,
               item:  1 ,
               thumbItem:  5 ,
               slideMargin:  0 ,
               currentPagerPosition: &#34;left&#34;,
               slideMove:  1 ,
               easing: &#34;cubic-bezier(0.25, 0, 0.25, 1)&#34;,
               speed:  600 ,
               responsive : [
               
                    
                    {
                        breakpoint:  800 ,
                        
                        settings: {
                            item:  3 ,
                            slideMove:  1 ,
                            slideMargin:  6 ,
                        }
                        
                    },
               
                    
                    {
                        breakpoint:  480 ,
                        
                        settings: {
                            item:  2 ,
                            slideMove:  1 ,
                            
                        }
                        
                    },
               
               ],
               onSliderLoad: function() {
                   $(&#34;#lightslider_example_slider1&#34;).lightGallery();
               }
               });
            });
        &lt;/script&gt;
    

